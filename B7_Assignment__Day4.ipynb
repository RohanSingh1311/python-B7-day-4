        },
        "outputId": "fba631c3-209f-4dcc-ba45-f3f1f49acff2"
      },
      "source": [
        "lower=1042000\n",
        "upper=702648256\n",
        "\n",
        "for num in range(lower,upper+1):\n",
        "  order=len(str(num))\n",
        "  sum=0\n",
        "  temp=num\n",
        "  while temp>0:\n",
        "    digit=temp%10\n",
        "    sum+=digit**order\n",
        "    temp//=10\n",
        "\n",
        "  if num==sum:\n",
        "    break\n",
        "print(num)"
      ],
